{
    "@context": {
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "prov": "http://www.w3.org/ns/prov#",
        "sdo": "http://schema.org/",
        "rico": "https://www.ica.org/standards/RiC/ontology#",
        "foaf": "http://xmlns.com/foaf/0.1/",
        "dcmitype": "http://purl.org/dc/dcmitype/",        
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "Archival Fonds-Collection": {
            "@id": "rico:RecordSet"
        },
        "Audio (music)": {
            "@id": "sdo:MusicRecording"
        },
        "Audio (spoken)": {
            "@id": "sdo:AudioObject"
        },
        "Book-Brochure": {
            "@id": "sdo:Book"
        },
        "Data catalog": {
            "@id": "sdo:DataCatalog"
        },
        "Dataset": {
            "@id": "sdo:DataSet"
        },
        "Event page": {
            "@id": "sdo:Event"
        },
        "File (in dataset)": {
            "@id": "sdo:DigitalDocument"
        },
        "Image": {
            "@id": "sdo:ImageObject"
        },
        "Object (e.g., banner)": {
            "@id": "dcmitype:PhysicalObject"
        },
        "Serial": {
            "@id": "sdo:CreativeWorkSeries"
        },
        "Video (moving image)": {
            "@id": "sdo:VideoObject"
        },
        "Web page": {
            "@id": "sdo:Webpage"
        },            
        "Creator": {
            "@id": "sdo:creator"
        }     
    }
